generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model ReadingResult {
  id        String   @id @default(uuid())
  data      String
  metadata  String?
  createdAt DateTime @default(now())
}
